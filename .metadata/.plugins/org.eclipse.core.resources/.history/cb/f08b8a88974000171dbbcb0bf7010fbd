package monopoli;

public class inPrigione extends Casella{

	inPrigione(int p, String n) {
		super(p, n);
		posizione = p; nome = n;
	}

	@Override
	void attivaEffetto(Giocatore g) {
		// TODO Auto-generated method stub
		g.setPrigion();
	}

	@Override
	String getNome() {
		// TODO Auto-generated method stub
		return null;
	}

}
