package monopoli;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

public class PedinaTest {
	Pedina p; Tabellone tab;
	@Before
	public void setUp() throws Exception {
		p = new Pedina("fungo");
		p.setProprietario(new Giocatore("d","h"));
		tab = new Tabellone();
	}

	@Test
	public void test() {
		p.muovi();
		assertEquals(p.getPosizione().getNome(), tab.getCasella(p.getPos()).getNome());
	}

}
